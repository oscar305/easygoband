            </div>
        {% else %}
            <div class="switch_lang relative">
              <div href="{{base_url}}/en" class="mr-2 text-lg uppercase no-underline translate">EN</div>
              <a href="{{base_url}}" class="mr-2 text-lg uppercase no-underline absolute">ES</a>
            </div>
        {% endif %}
