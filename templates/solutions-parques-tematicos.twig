                    <p><img src="{{assetPath('images/home_img/tick_white.png')}}">{{ lang == 'en' ? 'Update data and preferences': 'Actualizar datos y preferencias' }} </p>
                    <p><img src="{{assetPath('images/home_img/tick_white.png')}}">{{ lang == 'en' ? 'Fill out questionnaires': 'Rellenar cuestionarios' }} </p>
